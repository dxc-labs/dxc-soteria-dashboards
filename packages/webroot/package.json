  "scripts": {
    "start-js": "react-scripts start;#",
    "build-js": "react-scripts build;#",
    "mkdirs": "mkdir -p build/documents;#",
    "copy:assets": "cp -r src/assets/files/* build/documents;#",
    "copy": "npm run copy:assets;#",
    "delnodes": "rimraf node_modules;#",
    "start": "run-s setVar:* start-js;#",
    "build": "run-s setVar:* build-js && npm run mkdirs && npm run copy && npm run delnodes;#",
    "setVar": "run-s setVar:*",
    "setVar:init": "printf '' > .env",
    "setVar:checkpointtokenapi": "echo REACT_APP_CHECKPOINT_TOKEN_API=https://api-badge.example.com/auth/tokens >> .env",
    "setVar:checkpintqrurl": "echo REACT_APP_CHECKPOINT_QRURL=https://example.com/cp/ >> .env",
    "setVar:checkpointqrfilename": "echo REACT_APP_CHECKPOINT_QR_FILENAME=DXC-CheckpointQR >> .env",
    "setVar:checkpointvisitorsurveudomain": "echo REACT_APP_CHECKPOINT_VISITOR_SURVEY_DOMAIN=https://survey.example.com/visitorJoin/ >> .env",
    "setVar:checkpointvisitorsurveytoken": "echo REACT_APP_CHECKPOINT_VISITOR_SURVEY_TOKEN=xxx >> .env",
    "setVar:checkpointvisitorfilename": "echo REACT_APP_CHECKPOINT_VISITOR_FILENAME=DXC-VisitorNotice >> .env",
    "setVar:apidomain": "echo REACT_APP_API_DOMAIN=https://$(aws cloudformation list-exports --query 'Exports[?Name==`'$npm_config_project'-'$npm_config_tenant'-'$npm_config_environment'-api-domainName`].Value' --output text)/ >> .env",
    "setVar:userdomain": "echo REACT_APP_USER_DOMAIN=https://$(aws cloudformation list-exports --query 'Exports[?Name==`'$npm_config_project'-'$npm_config_tenant'-'$npm_config_environment'-domains-UserDomainName`].Value' --output text)/ >> .env",
    "setVar:cognitoUserPoolId": "echo REACT_APP_DASHBOARD_COGNITO_USERPOOL_ID=$(aws cloudformation list-exports --query 'Exports[?Name==`'$npm_config_project'-'$npm_config_tenant'-'$npm_config_environment'-identity-user-pool-id`].Value' --output text) >> .env",
    "setVar:cognitoUserPoolClientId": "echo REACT_APP_DASHBOARD_COGNITO_WEB_CLIENT_ID=$(aws cloudformation list-exports --query 'Exports[?Name==`'$npm_config_project'-'$npm_config_tenant'-'$npm_config_environment'-identity-user-pool-client-id`].Value' --output text) >> .env",
    "setVar:projectname": "echo REACT_APP_PROJECT=${npm_config_project} >> .env",
    "setVar:tenantname": "echo REACT_APP_TENANT=${npm_config_tenant} >> .env",
    "setVar:environment": "echo REACT_APP_ENV=${npm_config_environment} >> .env",
    "setVar:combo": "echo REACT_APP_COMBO= ${npm_config_project}-${npm_config_tenant}-${npm_config_environment} >> .env"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
